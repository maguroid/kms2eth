{
  "name": "kms2eth",
  "version": "0.1.3",
  "description": "Convert KMS keys to Ethereum addresses",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/maguroid/kms2eth.git"
  },
  "type": "module",
  "bin": {
    "kms2eth": "src/index.ts"
  },
  "scripts": {
    "build": "bun build src/index.ts --outfile dist/index.js --target node",
    "compile": "bun build src/index.ts --compile --outfile kms2eth --target node",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "test": "bun test"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.797.0",
    "@aws-sdk/client-sts": "^3.797.0",
    "@aws-sdk/credential-provider-node": "^3.797.0",
    "@noble/hashes": "^1.8.0",
    "@types/node": "^22.15.2",
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
